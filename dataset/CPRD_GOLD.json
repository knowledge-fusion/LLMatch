                "Description": "Date of registration with Child Health Surveillance",
                "Mapping": "DD/MM/YYYY",
                "Type": "DATE",
                "Format": "DD/MM/YYYY"
            },
            {
                "Column name": "Prescription Exemption",
                "Field name": "prescr",
                "Description": "Type of prescribing exemption the patient has currently (e.g. medical / maternity)",
                "Mapping": "Lookup PEX",
                "Type": "INTEGER",
                "Format": "3"
            },
            {
                "Column name": "Capitation Supplement",
                "Field name": "capsup",
                "Description": "Level of capitation supplement the patient has currently (e.g. low, medium, high)",
                "Mapping": "Lookup CAP",
                "Type": "INTEGER",
                "Format": "3"
            },
            {
                "Column name": "First Registration Date",
                "Field name": "frd",
                "Description": "Date the patient first registered with the practice. If patient only has \u2018temporary\u2019 records, the date is the first encounter with the practice; if patient has \u2018permanent\u2019 records it is the date of the first \u2018permanent\u2019 record (excluding preceding temporary records)",
                "Mapping": "DD/MM/YYYY",
                "Type": "DATE",
                "Format": "DD/MM/YYYY"
            },
            {
                "Column name": "Current Registration Date",
                "Field name": "crd",
                "Description": "Date the patient\u2019s current period of registration with the practice began (date of the first \u2018permanent\u2019 record after the latest transferred out period). If there are no \u2018transferred out periods\u2019, the date is equal to \u2018frd\u2019",
                "Mapping": "DD/MM/YYYY",
                "Type": "DATE",
                "Format": "DD/MM/YYYY"
            },
            {
                "Column name": "Registration Status",
                "Field name": "regstat",
                "Description": "Status of registration detailing gaps and temporary patients",
                "Mapping": "PAT_STAT1",
                "Type": "INTEGER",
                "Format": "2"
            },
            {
                "Column name": "Registration Gaps",
                "Field name": "reggap",
                "Description": "Number of days missing in the patient\u2019s registration details",
                "Mapping": "PAT_GAP2",
                "Type": "INTEGER",
                "Format": "5"
            },
            {
                "Column name": "Internal Transfer",
                "Field name": "internal",
                "Description": "Number of internal transfer out periods, in the patient\u2019s registration details",
                "Mapping": "None",
                "Type": "INTEGER",
                "Format": "2"
            },
            {
                "Column name": "Transfer Out Date",
                "Field name": "tod",
                "Description": "Date the patient transferred out of the practice, if relevant. Empty for patients who have not transferred out",
                "Mapping": "DD/MM/YYYY",
                "Type": "DATE",
                "Format": "DD/MM/YYYY"
            },
            {
                "Column name": "Transfer Out Reason",
                "Field name": "toreason",
                "Description": "Reason the patient transferred out of the practice. Includes 'Death' as an option",
                "Mapping": "Lookup TRA",
                "Type": "INTEGER",
                "Format": "3"
            },
            {
                "Column name": "Death Date",
                "Field name": "deathdate",
                "Description": "Date of death of patient \u2013 derived using a CPRD algorithm",
                "Mapping": "DD/MM/YYYY",
                "Type": "DATE",
                "Format": "DD/MM/YYYY"
            },
            {
                "Column name": "Acceptable Patient Flag",
                "Field name": "accept",
                "Description": "Flag to indicate whether the patient has met certain quality standards: 1 = acceptable, 0 = unacceptable",
                "Mapping": "Boolean",
                "Type": "INTEGER",
                "Format": "1"
            }
        ]
    },
